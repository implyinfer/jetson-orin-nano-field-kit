# Roboflow Inference Configuration
# Copy this file to .env and update with your settings
# Usage: cp .env.example .env

# Roboflow API Key (required)
# Get your API key from: https://app.roboflow.com/settings/api
ROBOFLOW_API_KEY=your_api_key_here

# RTSP Stream Configuration
# Local stream (if RTSP server is on same device)
RTSP_URL=rtsp://localhost:8554/cam0

# Remote stream (if RTSP server is on different device)
# RTSP_URL=rtsp://192.168.1.171:8554/cam0

# Alternative camera
# RTSP_URL=rtsp://localhost:8554/cam1

# Model Configuration
# Available aliases: yolov8n-640, yolov8n-1280, yolov8m-640
# Or use custom model ID from Roboflow: my-project/1
MODEL_ALIAS=yolov8n-640

# Detection Confidence Threshold (0.0 - 1.0)
# Lower = more detections but more false positives
# Higher = fewer detections but higher confidence
CONFIDENCE=0.5

# Display Configuration
# X Display to use (usually :0 for primary display)
DISPLAY=:0

# Optional: Suppress model dependency warnings
CORE_MODEL_SAM_ENABLED=False
CORE_MODEL_SAM2_ENABLED=False
CORE_MODEL_GAZE_ENABLED=False
CORE_MODEL_GROUNDINGDINO_ENABLED=False
